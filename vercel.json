{
  "builds": [
    {
      "src": "django_app_name/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "migrate_modelDB.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "django_app_name/wsgi.py"
    }
  ]
}